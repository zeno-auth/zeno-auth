<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="zeno_auth.event_listener.container" class="ZenoAuth\Infrastructure\Symfony\Bundle\ZenoAuthDoctrineBundle\EventListener\ContainerListener" public="false">
            <argument id="service_container" type="service"/>
            <tag name="doctrine.event_listener" event="getContainer"/>
        </service>
    </services>
</container>
