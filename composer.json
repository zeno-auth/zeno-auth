{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "borobudur/borobudur": "~0.1",
        "borobudur/doctrine-infra": "~0.1",
        "borobudur/symfony-infra": "~0.1",
        "doctrine/doctrine-migrations-bundle": "^1.3@dev",
        "sensio/framework-extra-bundle": "~3.0",
        "symfony/asset": "^3.3",
        "symfony/console": "^4.0@dev",
        "symfony/framework-bundle": "^3.3",
        "symfony/security-bundle": "^3.3",
        "symfony/serializer": "^3.4",
        "symfony/translation": "^4.0@dev",
        "symfony/twig-bundle": "^3.3",
        "symfony/var-dumper": "^4.0@dev",
        "symfony/web-server-bundle": "^3.3",
        "symfony/webpack-encore-pack": "dev-master",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "symfony/dotenv": "^3.3",
        "symfony/flex": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "ZenoAuth\\Web\\": "app/web/ZenoAuth",
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BXY3GAG0CBGD2QSHJ9DJCHKQ",
            "allow-contrib": false
        }
    }
}
